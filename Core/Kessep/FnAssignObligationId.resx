   <data name="ReadObligation">
      <value>
select top(1)
  CPA_TYPE,
  CSP_NUMBER,
  OB_ID,
  DTY_GENERATED_ID
from
  CKT_OBLIGATION
where
  (CPA_TYPE = 'R') and
  (CSP_NUMBER = @cspNumber)
order by
  OB_ID desc
</value>
