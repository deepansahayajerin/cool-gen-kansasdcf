<?xml version="1.0" encoding="utf-8"?>
<root>
   <resheader name="resmimetype">
      <value>text/microsoft-resx</value>
   </resheader>
   <resheader name="version">
      <value>2.0</value>
   </resheader>
   <resheader name="reader">
      <value>System.Resources.ResXResourceReader</value>
   </resheader>
   <resheader name="writer">
      <value>System.Resources.ResXResourceWriter</value>
   </resheader>
   <data name="DeleteCashReceiptRlnRsn">
      <value>
delete from CKT_CR_RLN_RSN
where
  CR_RLN_RSN_ID = @crRlnRsnId
</value>
   </data>
   <data name="ReadCashReceiptRlnRsn">
      <value>
select top(1)
  CR_RLN_RSN_ID,
  CODE,
  NAME,
  EFFECTIVE_DATE,
  DISCONTINUE_DATE,
  CREATED_BY,
  CREATED_TIMESTAMP,
  LAST_UPDATED_BY,
  LAST_UPDATED_TMST,
  DESCRIPTION
from
  CKT_CR_RLN_RSN
where
  CR_RLN_RSN_ID = @crRlnRsnId
</value>
   </data>
</root>
