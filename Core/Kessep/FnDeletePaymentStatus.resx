   <data name="DeletePaymentStatus">
      <value>
delete from CKT_PAYMENT_STATUS
where
  PAYMENT_STATUS_ID = @paymentStatusId
</value>
   </data>
   <data name="ReadPaymentStatus">
      <value>
select top(1)
  PAYMENT_STATUS_ID,
  CODE,
  NAME,
  EFFECTIVE_DATE,
  DISCONTINUE_DATE,
  CREATED_BY,
  CREATED_TIMESTAMP,
  LAST_UPDATE_BY,
  LAST_UPDATE_TMST,
  DESCRIPTION
from
  CKT_PAYMENT_STATUS
where
  PAYMENT_STATUS_ID = @paymentStatusId
</value>
   </data>
