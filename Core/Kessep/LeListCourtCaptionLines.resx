   <data name="ReadCourtCaption">
      <value>
select
  LGA_IDENTIFIER,
  COURT_CAPTION_NO,
  LINE
from
  CKT_COURT_CAPTION
where
  LGA_IDENTIFIER = @lgaIdentifier
order by
  COURT_CAPTION_NO
</value>
