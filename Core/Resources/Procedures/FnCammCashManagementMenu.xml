<procedure xmlns="http://www.bphx.com/coolgen/procedure/2008-11-04"
           name="FnCammCashManagementMenu"
           program-name="SWECAMMP"
           class="Gov.Kansas.DCF.Cse.Kessep.FnCammCashManagementMenu"
           transaction="SRIQ"
           display-first="false"
           unformatted-input="Command">
   <on exit-state="ECO_LNK_TO_CASH_RECEIPT"
       action="transfer"
       to="FnCrecCashReceipting"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_LNK_LST_CASH_RECEIPTS"
       action="transfer"
       to="FnCrelLstCashReceipts"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_XFR_TO_CRTB"
       display-first="true"
       action="transfer"
       to="FnCrtbBalCashRecptToTape"
       send-command-type="none"/>
   <on exit-state="ECO_LNK_RETURN_TO_MENU"
       display-first="true"
       action="transfer"
       to="FnFmenFnclMgmtMainMenu"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_LNK_LST_DEPOSITS"
       action="transfer"
       to="FnCrdpLstDeposits"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_LNK_LST_CR_DETAIL"
       action="transfer"
       to="FnCrdlLstCashReceiptDetails"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_LNK_LST_SEL_REFUNDS"
       display-first="true"
       action="transfer"
       to="FnCrrlListRefunds"
       send-command-type="none"/>
   <on exit-state="ECO_LNK_LST_UNDISTR_COLLCTN"
       action="transfer"
       to="FnCrucLstUndistrbtdCollectns"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_LNK_LST_ADVANCEMENTS"
       action="transfer"
       to="FnCralLstAdvancements"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_LNK_TO_OFFSET_ADVANCEMENT"
       action="transfer"
       to="FnCraoOffsetAdvancement"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_LNK_TO_REFUND_COLLECTION"
       action="transfer"
       to="FnCrruRefundUndstrbtdColl"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_XFR_TO_RECORD_MISC_REFUNDS"
       action="transfer"
       to="FnRmsrRequestMiscRefunds"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_LNK_MTN_CR_SOURCE_ASSGN"
       action="transfer"
       to="FnCramMtnCashSrceAssignment"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_XFR_TO_REC_CRRC"
       display-first="true"
       action="transfer"
       to="FnCrrcRecCollctnDetailsPri"
       send-command-type="none"/>
   <on exit-state="ECO_LNK_TO_BALANCE_INTERFACE"
       action="transfer"
       to="FnCrbiLstCrInterfaceBalance"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_LNK_LST_EFT_TRAN_INFO"
       action="transfer"
       to="FnLeftListElecFundTran"
       send-command="DISPLAY">
      <map from="EftTransmissionFileInfo.TransmissionType"
           to="Search.TransmissionType"/>
   </on>
   <on exit-state="ECO_LNK_LST_EFT_TRANSMISSION"
       action="transfer"
       to="FnLtrnListEftTransmissions"
       send-command="DISPLAY">
      <map from="ElectronicFundTransmission.TransmissionStatusCode"
           to="Search.TransmissionStatusCode"/>
      <map from="ElectronicFundTransmission.TransmissionType"
           to="Search.TransmissionType"/>
      <map from="ElectronicFundTransmission.FileCreationDate"
           to="Search.FileCreationDate"/>
   </on>
   <map from="Common" to="Common"/>
   <map from="Standard.NextTransaction" to="Standard.NextTransaction"/>
</procedure>
