<procedure xmlns="http://www.bphx.com/coolgen/procedure/2008-11-04"
           name="GbBchmBatchProcessingMenu"
           program-name="SWEBCHMP"
           class="Gov.Kansas.DCF.Cse.Kessep.GbBchmBatchProcessingMenu"
           transaction="SRMC"
           unformatted-input="Command">
   <on exit-state="GB0000_MTN_JCL"
       display-first="true"
       action="transfer"
       to="GbMjclMaintainJclTemplate"
       send-command-type="none"/>
   <on exit-state="CO_XFR_TO_PREQ"
       action="transfer"
       to="CoPreqPrintRequestStatus"
       send-command="DISPLAY"/>
   <on exit-state="ECO_XFR_TO_LST_JOB_RUNS"
       display-first="true"
       action="transfer"
       to="GbLjrnListJobRuns"
       send-command-type="none"/>
   <on exit-state="ECO_XFR_TO_LST_JOBS"
       action="transfer"
       to="GbLjobListJobs"
       send-command="DISPLAY"/>
   <on exit-state="ECO_LNK_RETURN_TO_MENU"
       action="transfer"
       to="CoCsmmChildSupMainMenu"
       send-command="XXFMMENU"/>
   <on exit-state="ECO_XFR_TO_LST_PGM_CHKPNT_RSTRT"
       action="transfer"
       to="GbLpcrLstPgmChkptRestart"
       send-command="DISPLAY"/>
   <on exit-state="ECO_XFR_TO_LST_PGM_PROCESS_INFO"
       action="transfer"
       to="GbLppiLstPgmProcessingInfo"
       send-command="DISPLAY"/>
   <on exit-state="ECO_MTN_TO_PGM_CHKPNT_RSTRT"
       display-first="true"
       action="transfer"
       to="GbMpcrMtnPgmChkptRestart"
       send-command-type="none"/>
   <on exit-state="ECO_XFR_TO_MTN_PGM_PROCESS_INFO"
       display-first="true"
       action="transfer"
       to="GbMppiMtnPgmProcessingInfo"
       send-command-type="none"/>
   <on exit-state="ECO_LNK_TO_LIST_SCREEN"
       display-first="true"
       action="transfer"
       to="GbLpniLstPgmNameInfo"
       send-command-type="none"/>
   <on exit-state="ECO_LNK_TO_MPNI"
       display-first="true"
       action="transfer"
       to="GbMpniMtnPgmName"
       send-command-type="none"/>
   <on exit-state="GB0000_MTN_JOB"
       display-first="true"
       action="transfer"
       to="GbMjobMaintainJob"
       send-command-type="none"/>
   <map from="Standard" to="Standard"/>
</procedure>
