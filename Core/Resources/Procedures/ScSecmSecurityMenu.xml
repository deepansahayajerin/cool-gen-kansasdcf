<procedure xmlns="http://www.bphx.com/coolgen/procedure/2008-11-04"
           name="ScSecmSecurityMenu"
           program-name="SWESECMP"
           class="Gov.Kansas.DCF.Cse.Security1.ScSecmSecurityMenu"
           transaction="SRGZ">
   <on exit-state="ECO_XFR_2_SC_ASGN_SRV_PRV_2_PRO"
       display-first="true"
       action="transfer"
       to="ScAsrvAssignServProvToProf"
       send-command="DISPLAY"/>
   <on exit-state="ECO_XFR_TO_SC_MAINTAIN_PROFILES"
       action="transfer"
       to="ScProfMaintainProfiles"
       send-command="DISPLAY"/>
   <on exit-state="ECO_XFR_TO_SC_MAINTAIN_COMMANDS"
       action="transfer"
       to="ScCmdsMaintainCommands"
       send-command="DISPLAY"/>
   <on exit-state="ECO_XFR_SC_ASGN_PRO_2_TRNS_CMDS"
       display-first="true"
       action="transfer"
       to="ScAtrnAssignTranCmdsToPrfs"
       send-command="DISPLAY"/>
   <on exit-state="ECO_XFR_SC_ASGN_SRV_PRV_2_PRO_S"
       display-first="true"
       action="transfer"
       to="ScAsruAsgnServprvProfByUsr"
       send-command-type="none"/>
   <on exit-state="ECO_XFR_2_SC_ASSIGN_CMDS_2_TRAN"
       display-first="true"
       action="transfer"
       to="ScAcmdAssignCommandsToTrans"
       send-command="DISPLAY"/>
   <on exit-state="ECO_XFR_TO_SC_MAINTAIN_TRANSCTN"
       action="transfer"
       to="ScTranMaintainTransaction"
       send-command="DISPLAY"/>
   <map from="Standard" to="Standard"/>
</procedure>
